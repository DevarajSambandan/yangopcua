<?xml version="1.0" encoding="utf-8"?>
<opc:ModelDesign
    xmlns:uax="http://opcfoundation.org/UA/2008/02/Types.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ua="http://opcfoundation.org/UA/"
    xmlns:opc="http://opcfoundation.org/UA/ModelDesign.xsd"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="urn:3gpp:sa5:_3gpp-5g-common-yang-types"
    TargetNamespace="urn:3gpp:sa5:_3gpp-5g-common-yang-types"
    TargetVersion="1.0">

    <opc:Namespaces>
        <opc:Namespace Name="_3gpp-5g-common-yang-types" Prefix="type5g3gpp" XmlPrefix="type5g3gpp">urn:3gpp:sa5:_3gpp-5g-common-yang-types</opc:Namespace>
        <opc:Namespace Name="_3gpp-common-yang-types" Prefix="types3gpp" XmlPrefix="types3gpp">http://opcfoundation.org/UA/default/</opc:Namespace>
    </opc:Namespaces>


	<opc:ObjectType SymbolicName="SnssaiType" BaseType="ua:BaseObjectType">
      <opc:Children>
        <opc:Property SymbolicName="Sd" DataType="ua:String" AccessLevel="Read"> 
          <opc:Description>Slice Differentiator
If not needed, the value can be set to FFFFFF.</opc:Description>
        </opc:Property>
        <opc:Property SymbolicName="Sst" DataType="ua:Byte" AccessLevel="Read"> 
          <opc:Description>Slice/Service Type.
Values 0 to 127 belong to standardized SST range and are defined in
3GPP TS 23.501. Values 128 to 255 belong to operator-specific range.</opc:Description>
        </opc:Property>
      </opc:Children>
    </opc:ObjectType>
	

    <opc:DataType SymbolicName="CommmodeltypeType" BaseType="ua:Enumeration"> 
      <opc:Fields>
        <opc:Field Name="enum DIRECT_COMMUNICATION_WO_NRF"  Identifier="0"  Description="Directly communicate to other pre-configured NF service." />
        <opc:Field Name="enum DIRECT_COMMUNICATION_WITH_NRF"  Identifier="1"  Description="Directly communicate to other NF service discovered
by NRF." />
        <opc:Field Name="enum INDIRECT_COMMUNICATION_WO_DEDICATED_DISCOVERY"  Identifier="2"  Description="Communicate to pre-configured other NF service through
SCP as a proxy." />
        <opc:Field Name="enum INDIRECT_COMMUNICATION_WITH_DEDICATED_DISCOVERY"  Identifier="3"  Description="Communication to NF service discovered by NRF through SCP
as a proxy." />
      </opc:Fields>
    </opc:DataType>
	
	<opc:ObjectType SymbolicName="SupportedfuncType" BaseType="ua:BaseObjectType">
      <opc:Children>
        <opc:Property SymbolicName="Function" DataType="ua:String" AccessLevel="Read"> 
        </opc:Property>
        <opc:Property SymbolicName="Policy" DataType="ua:String" AccessLevel="Read"> 
        </opc:Property>
      </opc:Children>
    </opc:ObjectType>
	
	
	<opc:ObjectType SymbolicName="CommmodelType" BaseType="ua:BaseObjectType">
      <opc:Children>
        <opc:Property SymbolicName="Groupid" DataType="ua:UInt16" AccessLevel="Read"> 
        </opc:Property>
        <opc:Property SymbolicName="Commmodeltype" DataType="CommmodeltypeType" AccessLevel="Read"> 
        </opc:Property>
        <opc:Property SymbolicName="Targetnfservicelist" DataType="CommmodeltypeType" AccessLevel="Read"> 
        </opc:Property>
        <opc:Property SymbolicName="Commmodelconfiguration" DataType="ua:String" AccessLevel="Read"> 
        </opc:Property>
      </opc:Children>
    </opc:ObjectType>

    <opc:DataType SymbolicName="Energysavingloadthresholdt" BaseType="ua:UInt32"> 
    </opc:DataType>
    <opc:DataType SymbolicName="Energysavingtimedurationt" BaseType="ua:UInt32"> 
    </opc:DataType>
    <opc:DataType SymbolicName="Physcellid" BaseType="ua:UInt32"> 
    </opc:DataType>
    <opc:DataType SymbolicName="Utc24timeofdayt" BaseType="ua:String"> 
      <opc:Description>Time of day in HH:MM or H:MM 24-hour format per UTC
time zone.</opc:Description>
    </opc:DataType>
    <opc:DataType SymbolicName="DayofweektType" BaseType="ua:Enumeration"> 
      <opc:Fields>
        <opc:Field Name="enum Monday" />
        <opc:Field Name="enum Tuesday" />
        <opc:Field Name="enum Wednesday" />
        <opc:Field Name="enum Thursday" />
        <opc:Field Name="enum Friday" />
        <opc:Field Name="enum Saturday" />
        <opc:Field Name="enum Sunday" />
      </opc:Fields>
    </opc:DataType>
</opc:ModelDesign>